def gv
pipeline{
  agent any
  stages{
  
  stage("init"){
      steps{
        script{
          gv = load "jenkinscript.groovy"
        }
      }
    }  
  stage("Build"){
      steps{
        echo "Building app"
      }
    }
    stage("Test"){
      steps{
        echo "Testing app"
      }
    }
    stage("Deploy"){
      steps{
        echo "Deploying app"
      }
    }
  }
}
