def gv
pipeline{
  agent any
  tools{
    maven 'Maven 3.9.6'
  }
  stages{
    stage("init"){
      steps{
        script{
          gv = load "buildPipelineScript.groovy"
        }
      }
    }
    stage("Build Jar"){
      steps{
        script{
          gv.buildApp()
        }
      }
    }
    stage("Build image a"){
      steps{
        script{
         gv.buildANndDeploy()
        }
      }
    }
  }
}
