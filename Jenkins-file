pipeline{
  agent any
  stages{
    stage("Testing"){
      steps{
        echo "Running test!"
      }
    }
    stage("Building"){
      steps{
        echo "Building Application!"
      }
    }
    stage("Deploying"){
      steps{
        echo "Deploying Application"
      }
    }
  }
}
posts{
  always{ 
    echo "Process End!"
  }
  success{
    echo "Completed with success!"
  }
  failure{
    echo "failed!"
  }
}
