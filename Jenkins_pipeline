def gv
pipeline{
  agent any
  stages{
    stage("Init"){
      steps{
        gv = load "jenkins_pipeline_script.groovy"
      }
    }
    stage("Testing"){
      steps{
        gv.fnTest()
      }
    }
    stage("Building"){
      steps{
        gv.fnBuild()
      }
    }
    stage("Deploying"){
      steps{
        gv.fnDeploy()
      }
    }
  }
}
