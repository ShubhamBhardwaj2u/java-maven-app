def gv
pipeline{
  agent any
  stages{
    stage("Init"){
      steps{
        script{
          gv = load "jenkins_pipeline_script.groovy"
        }
      }
    }
    stage("Testing"){
      steps{
        script{
          gv.fnTest()
        }
      }
    }
    stage("Building"){
      steps{
        script{
          gv.fnBuild()
        }
      }
    }
    stage("Deploying"){
      steps{
        script{
          gv.fnDeploy()
        }
      }
    }
  }
}
