pipeline{
  agent any
  stages{
    stage("Testing"){
      steps{
        echo "Testing!"
      }
    }
    stage("Building"){
      steps{
        echo "Building!"
      }
    }
    stage("Deploying"){
      steps{
        echo "Deploying!"
      }
    }
  }

  post{
  always{
    echo "Finished!"
  }
  success{
    echo "Done!"
  }
  failure{
    echo "Failed!"
  }
}
}


