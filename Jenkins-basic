pipeline{
  agent any

    stages{
      stage("Testing"){
        steps{
          echo "Testing is running!"
        }
      }
      stage("Building"){
        steps{
          echo "Building is running!"
        }
      }
      stage("Deploying"){
        steps{
          echo "Deploying is running!"
        }
      }
    }

  post{
    always{
      steps{
        echo "Default always!"
      }
    }
    success{
      steps{
        echo "Sucess"
      }
    }
    failure{
      steps{
        echo "failed"
      }
    }
  }
}
